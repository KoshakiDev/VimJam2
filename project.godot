; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node2D",
"class": "Bullet",
"language": "GDScript",
"path": "res://src/components/Bullet.gd"
}, {
"base": "Resource",
"class": "BulletEmitter",
"language": "GDScript",
"path": "res://BulletEmitter.gd"
}, {
"base": "BulletEmitter",
"class": "BulletEmitterSingle",
"language": "GDScript",
"path": "res://src/components/bullet_emitters/BulletEmitterSingle.gd"
}, {
"base": "BulletEmitter",
"class": "BulletEmitterSpread",
"language": "GDScript",
"path": "res://src/components/bullet_emitters/BulletEmitterSpread.gd"
}, {
"base": "Position2D",
"class": "BulletSpawner",
"language": "GDScript",
"path": "res://src/components/BulletSpawner.gd"
} ]
_global_script_class_icons={
"Bullet": "",
"BulletEmitter": "",
"BulletEmitterSingle": "",
"BulletEmitterSpread": "",
"BulletSpawner": ""
}

[application]

config/name="VimJam2"
run/main_scene="res://src/main.tscn"
config/icon="res://icon.png"

[global]

background=false

[importer_defaults]

texture={
"compress/bptc_ldr": 0,
"compress/hdr_mode": 0,
"compress/lossy_quality": 0.7,
"compress/mode": 0,
"compress/normal_map": 0,
"detect_3d": true,
"flags/anisotropic": false,
"flags/filter": false,
"flags/mipmaps": false,
"flags/repeat": 0,
"flags/srgb": 2,
"process/HDR_as_SRGB": false,
"process/fix_alpha_border": true,
"process/invert_color": false,
"process/premult_alpha": false,
"size_limit": 0,
"stream": false,
"svg/scale": 1.0
}

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
environment/default_environment="res://default_env.tres"
